{
    "name": "authanram/markdown",
    "type": "package",
    "description": "...",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "league/commonmark": "^2.0",
        "symfony/yaml": "^5.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "fakerphp/faker": "^1.17",
        "nunomaduro/phpinsights": "^2.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-mock": "^1.0",
        "pestphp/pest-plugin-watch": "1.x-dev",
        "spatie/ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\Markdown\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
